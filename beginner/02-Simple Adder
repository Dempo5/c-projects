#include <stdio.h>

int main (void)
{
 printf("----THE SUM GAME----\n\n");
 printf("Welcome to the Sum Game!\n");
 printf("In this game, you will be prompted to enter two numbers.\n");

 printf(" Give me two numbers and i will tell you their sum.\n");

int num1, num2;

 scanf("%d", &num1);
scanf("%d", &num2);
 int sum = num1 + num2;
 printf("The sum of %d and %d is %d\n", num1, num2, sum);




return 0;
}
