#include <stdio.h>


void swap(int *a, int *b) {
  int temp = *a;
  *a = *b;
  *b = temp;
}


int main() {

int a;
int b;

printf("Enter value one: ");
scanf("%d", &a);

printf("Enter value two: ");
scanf("%d", &b);

printf("You chose %d and %d\n", a, b);


swap(&a, &b);

printf("I like %d and %d more", a, b);

return 0;

}
